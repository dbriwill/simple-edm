<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
						"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.ged.models.GedMessage" table="message">

		<id name="id" column="rowid">
			<generator class="native"></generator>
		</id>
		
		<property name="date" column="message_date"></property>
		<property name="label" column="label"></property>
		<property name="read" column="read"></property>
		<property name="messageLevel" column="level"></property>

	</class>

</hibernate-mapping>

